<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="likeCheckBox" parent="Widget.Material3.Button.IconButton">

        <item name="android:layout_marginStart">16dp</item>

        <item name="android:layout_marginTop">6dp</item>

        <item name="android:textColor">@color/like_checkbox</item>

        <item name="iconPadding">4dp</item>

        <item name="iconTint">@color/like_checkbox</item>

    </style>

    <style name="shareCheckBox" parent="Widget.Material3.Button.IconButton">

        <item name="android:layout_marginStart">128dp</item>

        <item name="android:layout_marginTop">6dp</item>

        <item name="android:textColor">@color/share_color_checkbox</item>

        <item name="iconPadding">4dp</item>

        <item name="iconTint">@color/share_color_checkbox</item>

    </style>

    <style name="menuCheckBox" parent="Widget.Material3.Button.IconButton">

        <item name="android:layout_marginEnd">4dp</item>

        <item name="android:layout_marginTop">6dp</item>

        <item name="iconTint">@color/menu_color</item>

    </style>




</resources>